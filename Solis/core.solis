
class Number

	function abs()
		
		if self < 0 then 
			return -self
		end 

		return self

	end

end

class Bool

	function toString()
		
		if self then 
			return "true"
		end

		return "false"
	end	

end 

class String
	
	function toString()
		return self
	end

end

function println(val)
	
	__c_printf(val.toString())

end
