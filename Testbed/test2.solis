

class TestClass

	static var num = 256;

	var x = 10 ** 2;
	var y = 15;

	static function print() 
		println("Called from static");
	end

	function instanceFunc()
		println("Called from instance");

		println(self.y);
	end

end


var test = TestClass();

println(test.x);
println(test.y);

test.x = 50;

println(test.x);

println("Static: ");

println(TestClass.num);

TestClass.num = 512;

println(TestClass.num);

TestClass.print();

test.instanceFunc();