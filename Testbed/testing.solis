

class Vector2
	
	var x = 0
	var y = 0

	Vector2()

	end

	static function init(sx, sy)
		var out = Vector2()

		out.x = sx
		out.y = sy

		return out
	end


end

function update(dt)
	
	var pos = Vector2.init(2, 2)

	println(dt)
	println(pos.x)
	println(pos.y)

end

